{
    "printWidth": 120,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "parser": "json"
            }
        }
    ],
    "arrowParens": "avoid"
}